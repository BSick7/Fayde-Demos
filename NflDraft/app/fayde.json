{
  "libs": {
    "exjs": {
      "exports": "exjs",
      "base": "lib/exjs",
      "path": "lib/exjs/dist/ex.min"
    },
    "fayde.controls": {
      "exports": "Fayde.Controls",
      "base": "lib/fayde.controls"
    }
  },
  "themes": {
    "exjs": "none",
    "http://schemas.wsick.com/fayde": {
      "path": "themes/fayde/Default.theme.xml"
    },
    "fayde.controls": {
      "path": "lib/<libname>/themes/Metro.theme.xml"
    }
  },
  "debug": {
    "warnMissingThemes": false,
    "warnBrokenPath": false
  }
}